{
  "name": "@raycast/utils",
  "version": "1.15.0",
  "description": "Set of utilities to streamline building Raycast extensions",
  "author": "Raycast Technologies Ltd.",
  "homepage": "https://developers.raycast.com/utils-reference",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module.js",
      "require": "./dist/main.js"
    }
  },
  "scripts": {
    "lint": "eslint src/",
    "build": "(rm -rf .parcel-cache || true) && (rm -rf dist || true) && parcel build",
    "dev": "parcel watch",
    "test": "npm run lint",
    "prepublishOnly": "npm run build && npm test",
    "postpublish": "gh workflow run sync-utils-docs.yml --repo raycast/extensions"
  },
  "peerDependencies": {
    "@raycast/api": ">=1.69.0",
    "react": "*"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "keywords": [
    "Raycast",
    "utilities",
    "extensions"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "license": "MIT",
  "dependencies": {
    "dequal": "^2.0.3",
    "node-fetch-cjs": "^3.3.2",
    "object-hash": "^3.0.0",
    "signal-exit": "^4.0.2",
    "stream-chain": "^2.2.5",
    "stream-json": "^1.8.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "@raycast/api": "1.52.0",
    "@types/content-type": "^1.1.6",
    "@types/media-typer": "^1.1.1",
    "@types/object-hash": "^3.0.4",
    "@types/signal-exit": "^3.0.2",
    "@types/stream-chain": "^2.0.4",
    "@types/stream-json": "^1.7.7",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "parcel": "^2.12.0",
    "typescript": "5.4.5"
  }
}
